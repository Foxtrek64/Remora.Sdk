<Project>
    <Import Project="Sdk.props" Sdk="Remora.Sdk" Version="1.0.0" />

    <!-- Override target frameworks -->
    <PropertyGroup>
        <TargetFrameworks>$([System.Text.RegularExpressions.Regex]::Replace($(LibraryFrameworks), 'netstandard.+?;', ''))</TargetFrameworks>
    </PropertyGroup>

    <!-- Override packability -->
    <PropertyGroup>
        <IsPackable>false</IsPackable>
    </PropertyGroup>
</Project>
